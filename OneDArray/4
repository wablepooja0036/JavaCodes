import java.util.Scanner;
class OneDarray7{
        public static void main(String []args){
                int empId[] =new Int [3];
                empId[0]=10;
                empId[1]=20;
                empId[2]=30;
                empId[3]=40;
                System.out.println(empId[0]);
                System.out.println(empId[1]);
                System.out.println(empId[2]);
                System.out.println(empId[3]);
        }
}
